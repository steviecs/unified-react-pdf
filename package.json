    "pdf builder",
    "parsable pdf",
    "searchable pdf",
    "html pdf builder"
  ],
  "main": "dist/index.js",
  "dependencies": {
    "@react-pdf/renderer": "^3.0.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-pdf": "5.7.2",
    "react-pdf-html": "1.1.14"
  },
  "devDependencies": {
    "@types/react": "17.0.0",
    "@types/react-dom": "17.0.0",
    "@types/react-pdf": "^5.7.3",
    "typescript": "^4.7.2"
  },
  "scripts": {
    "build": "rm -rf dist && tsc --noEmit false --declaration true --isolatedModules false"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "files": [
    "dist"
  ]
